		<script type="text/javascript">

		function btnrclick($temp)
		{
			$('#hiddencomid').val($temp);
			$('#commentsendr').fadeIn();
			event.stopPropagation();
			$('#shadowmkr').fadeIn();
		}

		function closecomment()
		{	
			$('#shadowmkr').fadeOut();
			$('#commentsendr').fadeOut();
		}

		function check1()
		{
			$flag=true;
			if($('#commenttext').val()==null || $('#commenttext').val()=="")
			{
				$('#commenttext').css('background-color','#ffcfab');
				$flag=false;
			}
			if($flag==false)
			{
				return false;
			}
		}

		function check2()
		{
			$flag=true;
			if($('#commenttextr').val()==null || $('#commenttextr').val()=="")
			{
				$('#commenttextr').css('background-color','#ffcfab');
				$flag=false;
			}
			if($flag==false)
			{
				return false;
			}
		}
		
		function resetcolor($temp)
		{
			$temp.css('background-color','#FFF');
		}
		
		</script>
