<div id="top" style="cursor:pointer; position:fixed; bottom:10px; right:20px;">
	<img id="backtotop" alt="top" width="25" height="25" style="display:none; cursor:pointer;
	  border:solid; border-width:1px; border-color:#033; background-color: #FFF; padding: 5px 5px 5px 5px; border-radius:5px 5px 5px 5px;" src="images/top.png" />
	<script>
		$(window).scroll(function() {
		    if($(this).scrollTop() > 200) {
		        $('#backtotop').fadeIn();
		    } else {
		        $('#backtotop').fadeOut();
		    }
		});
		
		$('#backtotop').bind('click', function(e) {
		    e.preventDefault();
		    $('body,html').animate({scrollTop:0},800);
		});
	</script>
</div>

