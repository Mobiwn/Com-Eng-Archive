		<script>
			$(window).scroll(function() {
			    if($(this).scrollTop() > 200) {
			        $('#backtotop').fadeIn();
			    } else {
			        $('#backtotop').fadeOut();
			    }
			});
			
			
			
			$('#backtotop').bind('click', function(e) {
			    e.preventDefault();
			    $('body,html').animate({scrollTop:0},800);
			});
		</script>
